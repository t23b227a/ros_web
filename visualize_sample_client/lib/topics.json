[
    {
        "name": "/rosout",
        "messageType": "rcl_interfaces/msg/Log",
        "show": "[${level}] ${name}: ${msg}"
    },
    {
        "name": "/goal_pose",
        "messageType": "geometry_msgs/PoseStamped",
        "show": "x: ${pose.position.x}, y: ${pose.position.y}"
    },
    {
        "name": "test_talker",
        "messageType": "std_msgs/String",
